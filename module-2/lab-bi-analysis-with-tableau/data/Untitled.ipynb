    "import csv  \n",
    "import json  \n",
    "  \n",
    "\n",
    "f = open( 'churn.csv', 'rU' )  \n",
    "  \n",
    "reader = csv.DictReader(f)  \n",
    "\n",
    "out = json.dumps( [ row for row in reader ] )  \n",
    "  \n",
    "  \n",
    "f = open( 'churn.json', 'w')  \n",
    "f.write(out)  \n"
